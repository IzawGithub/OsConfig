{
    "__COMMENT__ More Information": "https://github.com/mozilla/policy-templates/blob/master/README.md",
    "policies": {
        "AppUpdateURL": "https://localhost",
        "DisableAppUpdate": true,
        "OverrideFirstRunPage": "",
        "OverridePostUpdatePage": "",
        "DisableSystemAddonUpdate": true,
        "DisableProfileImport": false,
        "DisableFirefoxStudies": true,
        "DisableTelemetry": true,
        "DisableFeedbackCommands": true,
        "DisablePocket": true,
        "DisableSetDesktopBackground": false,
        "DisableDeveloperTools": false,
        "HardwareAcceleration": true,
        "NoDefaultBookmarks": true,
        "WebsiteFilter": {
            "Block": [
                "https://localhost/*"
            ],
            "Exceptions": [
                "https://localhost/*"
            ]
        },
        "Extensions": {
            "Install": [
                "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/darkreader/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/firefox-color/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/proton-pass/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/plasma-integration/latest.xpi"
            ],
            "Uninstall": [
                "google@search.mozilla.org",
                "bing@search.mozilla.org",
                "amazondotcom@search.mozilla.org",
                "ebay@search.mozilla.org",
                "twitter@search.mozilla.org"
            ]
        },
        "ExtensionsSettings": {
        },
        "SearchEngines": {
            "PreventInstalls": false,
            "Remove": [
                "Google",
                "Bing",
                "Amazon.com",
                "eBay",
                "Twitter"
            ],
            "Default": "DuckDuckGo",
            "Add": [
                {
                    "Name": "Arch Linux AUR",
                    "Description": "Searches for packages in the Arch User Repository",
                    "Alias": "@aur",
                    "Method": "GET",
                    "URLTemplate": "https://aur.archlinux.org/packages/?K={searchTerms}",
                    "IconURL": "data:image/x-icon;base64,"
                },
                {
                    "Name": "Arch Linux Packages",
                    "Description": "Searches for packages in the Arch Linux database",
                    "Alias": "@arch",
                    "Method": "GET",
                    "URLTemplate": "https://www.archlinux.org/packages/?q={searchTerms}",
                    "IconURL": "data:image/x-icon;base64,"
                },
                {
                    "Name": "Arch Linux Wiki",
                    "Description": "Searches the Arch Linux wiki",
                    "Alias": "@archwiki,@awiki",
                    "Method": "GET",
                    "URLTemplate": "https://wiki.archlinux.org/index.php/Special:Search?go=Go&search={searchTerms}",
                    "IconURL": "data:image/x-icon;base64,"
                },
                {
                    "Name": "Google",
                    "Description": "Google encrypted search, safe off, English",
                    "Alias": "@google,@g",
                    "Method": "GET",
                    "URLTemplate": "https://encrypted.google.com/search?hl=en&safe=off&pws=0&q={searchTerms}"
                }
            ]
        },
        "SupportMenu": {
            "Title": "LibreWolf Issue Tracker",
            "URL": "https://codeberg.org/librewolf/issues"
        }
    }
}
