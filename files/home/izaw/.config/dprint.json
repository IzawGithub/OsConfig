{
    "$schema": "https://dprint.dev/schemas/v0.json",
    "indentWidth": 4,
    "lineWidth": 100,
    "newLineKind": "lf",
    "useTabs": false,
    "dockerfile": {},
    "json": {},
    "jupyter": {},
    "malva": {
        "hexColorLength": "long",
        "declarationOrder": "smacss",
        "formatComments": "true",
        "trailingComma": "true"
    },
    "markdown": {
        "emphasisKind": "asterisks"
    },
    "markup": {
        "formatComments": "true",
        "scriptIndent": "true"
    },
    "ruff": {},
    "toml": {},
    "typescript": {},
    "yaml": {
        "indentWidth": 2,
        "bracketSpacing": true,
        "dashSpacing": "indent",
        "formatComments": true
    },
    "excludes": [
        "**/node_modules",
        "**/*-lock.json"
    ],
    "plugins": [
        "https://plugins.dprint.dev/dockerfile-0.3.3.wasm",
        "https://plugins.dprint.dev/g-plane/malva-v0.14.2.wasm",
        "https://plugins.dprint.dev/g-plane/markup_fmt-v0.23.1.wasm",
        "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm",
        "https://plugins.dprint.dev/json-0.20.0.wasm",
        "https://plugins.dprint.dev/jupyter-0.2.0.wasm",
        "https://plugins.dprint.dev/markdown-0.19.0.wasm",
        "https://plugins.dprint.dev/ruff-0.4.7.wasm",
        "https://plugins.dprint.dev/toml-0.7.0.wasm",
        "https://plugins.dprint.dev/typescript-0.95.10.wasm"
    ]
}
